{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
        "@tree-tails/core/*": ["../packages/core/src/*"],
        "@tree-tails/core": ["../packages/core/src"],
        "@tree-tails/mui/*": ["../packages/core/src/*"],
        "@tree-tails/mui": ["../packages/core/src"]
    }
  },
  "references": [
    {
      "path": "../packages/mui"
    }
  ]
}
