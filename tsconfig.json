{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "preserve",
    "moduleResolution": "Bundler",
    "paths": {
      "@tree-tails/core/*": ["./packages/core/src/*"],
      "@tree-tails/core": ["./packages/core/src"],
      "@tree-tails/mui/*": ["./packages/core/src/*"],
      "@tree-tails/mui": ["./packages/core/src"]
    }
  },
  "files": [],
  "references": [
    { "path": "./packages/core" },
    { "path": "./packages/mui" },
    { "path": "./storybook" }
  ]
}
